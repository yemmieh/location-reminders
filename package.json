{
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "lint": "./node_modules/tslint/bin/tslint -c tslint.json -p tsconfig.json --fix",
    "ios": "react-native run-ios",
    "web": "expo start --web",
    "local:init-db": "sh ./scripts/init-scripts/initialize-local-db.sh",
    "start": "concurrently \"NODE_ENV=local nodemon ./dist/index.js\" \"react-native start\"",
    "start_windows": "concurrently \"set NODE_ENV=local && nodemon ./dist/index.js\" \"react-native start\"",
    "dev": "nodemon src/app.ts",
    "build": "npm run lint && npm run build-server",
    "build-server": "./node_modules/typescript/bin/tsc -p .",
    "test": "jest"
  },
  "dependencies": {
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.165",
    "@types/node-fetch": "^2.5.7",
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "expo": "~39.0.2",
    "expo-splash-screen": "~0.6.2",
    "expo-status-bar": "~1.0.2",
    "expo-updates": "~0.3.2",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.0",
    "node-fetch": "^2.6.1",
    "patch-package": "^6.2.2",
    "react": "16.13.1",
