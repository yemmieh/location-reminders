    "react-native-screens": "~2.10.1",
    "react-native-unimodules": "~0.11.0",
    "react-native-web": "~0.13.12",
    "reflect-metadata": "^0.1.13",
    "typescript": "4.0.5",
    "winston": "^3.3.3",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@types/dotenv": "^8.2.0",
    "babel-jest": "~25.2.6",
    "jest": "~25.2.6",
    "react-test-renderer": "~16.13.1",
    "tslint": "^6.1.3"
